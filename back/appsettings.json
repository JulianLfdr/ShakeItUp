{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TheCocktailDB": {
    "BaseUrl": "https://thecocktaildb.com",
    "ApiPath": "api/json/v1",
    "ApiKey": "1",
    "Endpoints": {
      "GetRandom": {
        "Path": "random.php"
      },
      "SearchCocktail": {
        "Path": "search.php",
        "Parameters": {
          "ByName": "s",
          "ByFirstLetter": "s",
          "ByIngredientName": "i"
        }
      },
      "LookupCocktail": {
        "Path": "lookup.php",
        "Parameters": {
          "CocktailById": "i",
          "IngredientById": "ii"
        }
      }
    }
  }
}